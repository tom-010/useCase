<idea-plugin version="2">
	<extensions defaultExtensionNs="org.eclipse.xtext.idea">
		<package
			uri="http://www.deniffel.io/dsl/useCase/UseCase"
			class="io.deniffel.dsl.useCase.useCase.UseCasePackage"
		/>
		<resourceFactory 
			type="useCase"
			class="org.eclipse.xtext.resource.IResourceFactory"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
		/>
		<resourceServiceProvider
			uriExtension="useCase"
			class="org.eclipse.xtext.idea.resource.IResourceIdeaServiceProvider"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
		/>

		<lang.setup
			language="io.deniffel.dsl.useCase.UseCase"
			implementationClass="io.deniffel.dsl.useCase.idea.UseCaseIdeaSetup"
		/>
	</extensions>

	<extensions defaultExtensionNs="com.intellij">
		<psi.treeChangePreprocessor implementation="io.deniffel.dsl.useCase.idea.lang.psi.UseCaseCodeBlockModificationListener"/>

		<fileTypeFactory implementation="io.deniffel.dsl.useCase.idea.lang.UseCaseFileTypeFactory"/>
		<stubElementTypeHolder class="io.deniffel.dsl.useCase.idea.lang.UseCaseElementTypeProvider"/>
		<lang.ast.factory 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="org.eclipse.xtext.idea.lang.BaseXtextASTFactory"
		/>
		<lang.parserDefinition 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="io.deniffel.dsl.useCase.idea.lang.parser.UseCaseParserDefinition"
		/>
		<lang.findUsagesProvider 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="org.eclipse.xtext.idea.findusages.BaseXtextFindUsageProvider"
		/>
		<lang.refactoringSupport 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="org.eclipse.xtext.idea.refactoring.BaseXtextRefactoringSupportProvider"
		/>
		<lang.namesValidator 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="com.intellij.lang.refactoring.NamesValidator"
		/>
		<lang.syntaxHighlighterFactory key="io.deniffel.dsl.useCase.UseCase" implementationClass="io.deniffel.dsl.useCase.idea.highlighting.UseCaseSyntaxHighlighterFactory" />
		<lang.braceMatcher 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="com.intellij.lang.PairedBraceMatcher"
		/>
		<annotator 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="org.eclipse.xtext.idea.annotation.IssueAnnotator"
		/>
		<completion.contributor language="io.deniffel.dsl.useCase.UseCase" implementationClass="io.deniffel.dsl.useCase.idea.completion.UseCaseCompletionContributor"/>
		<pom.declarationSearcher implementation="io.deniffel.dsl.useCase.idea.lang.pom.UseCasePomDeclarationSearcher"/>

		<lang.psiStructureViewFactory 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="com.intellij.lang.PsiStructureViewFactory"
		/>
		<facetType implementation="io.deniffel.dsl.useCase.idea.facet.UseCaseFacetType"/>
		<lang.documentationProvider 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="org.eclipse.xtext.idea.documentation.IdeaDocumentationProvider"
		/>
		<colorSettingsPage implementation="io.deniffel.dsl.useCase.idea.highlighting.UseCaseColorSettingsPage"/>
		<highlightVisitor implementation="io.deniffel.dsl.useCase.idea.highlighting.UseCaseSemanticHighlightVisitor"/>

		<lang.formatter 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="com.intellij.formatting.FormattingModelBuilder"
		/>
		<lang.commenter 
			language="io.deniffel.dsl.useCase.UseCase"
			factoryClass="io.deniffel.dsl.useCase.idea.UseCaseExtensionFactory"
			implementationClass="com.intellij.lang.CodeDocumentationAwareCommenter"
		/>
	</extensions>
</idea-plugin>
